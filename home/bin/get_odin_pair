#!/apollo/bin/env -e WarehouseDealsPythonUtils python
import sys
from pyodinhttp import * 
 
mat_name=sys.argv[1]
  
try:
    principal=odin_material_retrieve(_materialName=mat_name, _materialType='Principal')
    credential=odin_material_retrieve(_materialName=mat_name, _materialType='Credential')
    print("Principal:%s" % (principal))
    print("Credential:%s" % (credential))

except OdinOperationError, err:
    print err
